        <th>
            @Html.DisplayNameFor(model => model.Brand.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EquipmentType.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Model.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Warehouse.Description)
        </th>
